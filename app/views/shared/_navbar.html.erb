<div class="container">
  <div class="navbar">
    <% if signed_in? %>
      <div>
        <%= link_to "Edit Details", edit_user_registration_path, class: "btn btn-outline-secondary my-1" %>
        <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: "btn btn-outline-secondary my-1" %>
        <%= link_to "Go back", root_path, class: "btn btn-outline-secondary my-1" %>
      </div>
      <div>
      <%= link_to cart_path(@current_cart), class: "btn btn-outline-danger my-1" do %>
        <%= content_tag :i, "", class: "fas fa-shopping-cart" %>
      <% end %>
      </div>
    <% end %>
  </div>
</div>
